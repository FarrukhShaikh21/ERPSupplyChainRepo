  <validation:ExpressionValidationBean
    Name="ScmPurchaseRfqHeaderBR"
    OperandType="EXPR"
    Inverse="false">
    <validation:OnAttributes>
      <validation:Item
        Value="DueDate"/>
      <validation:Item
        Value="CloseDate"/>
      <validation:Item
        Value="ValidFromDate"/>
      <validation:Item
        Value="ValidToDate"/>
      <validation:Item
        Value="LocationId"/>
      <validation:Item
        Value="ApprovalStatusSno"/>
      <validation:Item
        Value="TempDepartmentId"/>
      <validation:Item
        Value="TempProjectId"/>
    </validation:OnAttributes>
    <validation:OnCondition
      Name="OnConditionExpr"><![CDATA[IsSupervised=='N']]></validation:OnCondition>
    <validation:MsgIds>
      <validation:Item
        Value="ERR_PROJECT"/>
      <validation:Item
        Value="ERR_VALID_DATES"/>
      <validation:Item
        Value="ERR_LOCATIONID"/>
      <validation:Item
        Value="ERR_DUE_DATE_FUTURE"/>
      <validation:Item
        Value="ERR_DEPARTMENT"/>
      <validation:Item
        Value="ERR_CLOSE_DATE"/>
    </validation:MsgIds>
    <validation:TransientExpression><![CDATA[if(LocationId==null)
{
adf.error.raise("ERR_LOCATIONID");
return false;
}

if(TempDepartmentId==null)
{
adf.error.raise("ERR_DEPARTMENT");
return false;
}

if(TempProjectId==null)
{
adf.error.raise("ERR_PROJECT");
return false;
}

if(DueDate==adf.currentDate)
{
adf.error.raise("ERR_DUE_DATE_FUTURE");
return false;
}

if(CloseDate!=null && DueDate!=null && CloseDate<DueDate)
{
adf.error.raise("ERR_CLOSE_DATE");
return false;
}


if(ValidFromDate>ValidToDate)
{
adf.error.raise("ERR_VALID_DATES");
return false;
}


return true;]]></validation:TransientExpression>
  </validation:ExpressionValidationBean>
