      <validation:Item
        Value="ERR_REQ_REQUIRED_DATE"/>
    </validation:MsgIds>
    <validation:TransientExpression
      trustMode="untrusted"><![CDATA[if(ItemId==null)
{
adf.error.raise("ERR_REQ_ITEM_ID");
return false;
}
if(UnitTypeSno==null)
{
adf.error.raise("ERR_REQ_UNIT_TYPE");
return false;
}

if(AproxPrice==null)
{
adf.error.raise("ERR_REQ_APROX_PRICE");
return false;
}

if(AproxPrice==null)
{
adf.error.raise("ERR_REQ_DEMAND_QTY");
return false;
}

if(RequiredByDate==null)
{
adf.error.raise("ERR_REQ_REQUIRED_DATE");
return false;
}

if(RequiredByDate<=adf.currentDate)
{
adf.error.raise("ERR_REQ_FUTURE_DATE");
return false;
}

if(DemandQuantity==null ||DemandQuantity<=0)
{
adf.error.raise("ERR_REQ_DEMAND_QUANTITY");
return false;
}



if(txtIsDuplicateItem.equals("ERPYES") && 2==1)
{
adf.error.raise("ERR_REQ_DUPLICATE_ITEM");
return false;
}

if(InventoryOrgSno==null)
{
adf.error.raise("ERR_REQ_INVENTORY_ORG");
return false;
}

return true;]]></validation:TransientExpression>
