<?xml version="1.0" encoding="windows-1252" ?>
<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="CustomerTypeForTaxCatQVO"
  Version="12.1.3.2.1"
  InheritPersonalization="merge"
  BindingStyle="JDBC"
  CustomQuery="true"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="erpscm.modelscm.scmqvo.CustomerTypeForTaxCatQVO_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="P_ADF_COMPANY_ID"
    Kind="where"
    Type="java.lang.Integer"/>
  <Variable
    Name="P_ADF_TAX_TYPE_SNO"
    Kind="where"
    Type="java.lang.Integer"/>
  <SQLQuery><![CDATA[select ct.customer_type_code, 
     ct.customer_type_name, 
       ct.customer_type_short_name, 
       ct.customer_type_sno 
from scm_customer_type ct 
where ct.is_supervised='Y' 
AND  ct.is_active='Y' 
and  ct.company_id=? /*0*/ 
and not exists( 
        select '' 
         FROM scm_cust_type_tax X 
         WHERE ct.customer_type_sno=x.customer_type_sno 
         and   x.tax_type_sno=? /*1*/ 
          ) 
order by ct.customer_type_name]]></SQLQuery>
  <ViewAttribute
    Name="CustomerTypeCode"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INTEGER"
    AliasName="customer_type_code"
    Expression="customer_type_code"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerTypeName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="100"
    Type="java.lang.String"
    ColumnType="VARCHAR"
    AliasName="customer_type_name"
    Expression="customer_type_name"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="100"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerTypeShortName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR"
    AliasName="customer_type_short_name"
    Expression="customer_type_short_name"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerTypeSno"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Type="java.lang.Integer"
    ColumnType="INT"
    AliasName="customer_type_sno"
    Expression="customer_type_sno"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="5"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="CustomerTypeSno"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="erpscm.modelscm.ModelSCMBundle"/>
  </ResourceBundle>
</ViewObject>
