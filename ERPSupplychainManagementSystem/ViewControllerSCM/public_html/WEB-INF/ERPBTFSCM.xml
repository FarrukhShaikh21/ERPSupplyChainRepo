<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="ERPBTFSCM">
    <default-activity>doSetSCMErpApplicationGlobals</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>P_ADF_USER_SNO</name>
      <value>#{pageFlowScope.OracleERPBean.ERPuserId}</value>
      <class>erpscm.viewscm.ERPSCMClass</class>
    </input-parameter-definition>
    <input-parameter-definition id="__10">
      <name>P_ADF_USER_CODE</name>
      <value>#{pageFlowScope.OracleERPBean.ERPuserCode}</value>
      <class>erpscm.viewscm.ERPSCMClass</class>
    </input-parameter-definition>
    <input-parameter-definition id="__11">
      <name>P_ADF_USER_ATTRIBUTE</name>
      <value>#{pageFlowScope.OracleERPBean.ERPUserAttributes}</value>
      <class>erpscm.viewscm.ERPSCMClass</class>
    </input-parameter-definition>
    <managed-bean id="__12">
      <managed-bean-name>OracleERPBean</managed-bean-name>
      <managed-bean-class>erpscm.viewscm.ERPSCMClass</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__13">
      <managed-bean-name>ERPMainTempBean</managed-bean-name>
      <managed-bean-class>erptemplate.viewtemp.ERPTEMPClass</managed-bean-class>
      <managed-bean-scope>pageFlow</managed-bean-scope>
    </managed-bean>
    <method-call id="doSetSCMErpApplicationGlobals">
      <method>#{pageFlowScope.OracleERPBean.doSetSCMErpApplicationGlobals}</method>
      <outcome>
        <fixed-outcome>ACT-GOTOSCM-MENU</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ERPSCMSetupExeWithPara">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERPSCMParaTrans</fixed-outcome>
      </outcome>
    </method-call>
    <task-flow-return id="RETURNSCMRETURN">
      <outcome>
        <name>RETURNSCMRETURN</name>
      </outcome>
    </task-flow-return>
    <method-call id="ERPSCMTransExeWitPara">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERPSCMParaReports</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ERPSCMReportsExeWitPara">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERP-ACT-SCM_MAIN</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SCM_MAIN">
      <page>/SCM_MAIN.jsf</page>
    </view>
    <method-call id="doSCMGotoErpActivity">
      <method>#{pageFlowScope.ERPMainTempBean.doGotoErpActivity}</method>
      <outcome>
        <to-string/>
      </outcome>
    </method-call>
    <view id="SCM_0001">
      <page>/SCM_0001.jsf</page>
    </view>
    <view id="SCM_0001_EDIT">
      <page>/SCM_0001_EDIT.jsf</page>
    </view>
    <method-call id="ExecuteWithParSCMPARAByCompany">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ExecuteWithParamsCompanyWise">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SCM_0002">
      <page>/SCM_0002.jsf</page>
    </view>
    <method-call id="ExecuteWithParSCMPARAByComp">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExecuteWithParams</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SCM_0002_EDIT">
      <page>/SCM_0002_EDIT.jsf</page>
    </view>
    <view id="SCM_0003">
      <page>/SCM_0003.jsf</page>
    </view>
    <method-call id="ExeCompanySEC_0003">
      <method>#{bindings.Execute.execute}</method>
      <outcome>
        <fixed-outcome>ERP-ACT-SCM_0003</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SCM_0003_EDIT">
      <page>/SCM_0003_EDIT.jsf</page>
    </view>
    <method-call id="ExeParaCompanyByCompID">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERP-ACT-SCM_0003_QUERY</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ExeTermHeaderByCompany">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ExeTermHeaderByCompany</fixed-outcome>
      </outcome>
    </method-call>
    <method-call id="ExePayTermByPaymentHeaderID">
      <method>#{bindings.ExecuteWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERP-ACT-SCM_0003EDITQUERY</fixed-outcome>
      </outcome>
    </method-call>
    <view id="SCM_0003_EDIT_EDIT">
      <page>/SCM_0003_EDIT_EDIT.jsf</page>
    </view>
    <method-call id="CreateWithParaPaySCM_0003">
      <method>#{bindings.CreateWithParams.execute}</method>
      <outcome>
        <fixed-outcome>ERP-ACT-SCM_0003ADD</fixed-outcome>
      </outcome>
    </method-call>
    <control-flow-rule id="__2">
      <from-activity-id>doSetSCMErpApplicationGlobals</from-activity-id>
      <control-flow-case id="__3">
        <from-outcome>ACT-GOTOSCM-MENU</from-outcome>
        <to-activity-id>ERPSCMSetupExeWithPara</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__4">
      <from-activity-id>ERPSCMSetupExeWithPara</from-activity-id>
      <control-flow-case id="__5">
        <from-outcome>ERPSCMParaTrans</from-outcome>
        <to-activity-id>ERPSCMTransExeWitPara</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__6">
      <from-activity-id>ERPSCMTransExeWitPara</from-activity-id>
      <control-flow-case id="__7">
        <from-outcome>ERPSCMParaReports</from-outcome>
        <to-activity-id>ERPSCMReportsExeWitPara</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__8">
      <from-activity-id>ERPSCMReportsExeWitPara</from-activity-id>
      <control-flow-case id="__9">
        <from-outcome>ERP-ACT-SCM_MAIN</from-outcome>
        <to-activity-id>SCM_MAIN</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__14">
      <from-activity-id>*</from-activity-id>
      <control-flow-case id="__15">
        <from-outcome>doStartERPTaskFlow</from-outcome>
        <to-activity-id>doSCMGotoErpActivity</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__22">
      <from-activity-id>doSCMGotoErpActivity</from-activity-id>
      <control-flow-case id="__23">
        <from-outcome>ERP-ACT-SCM_0001</from-outcome>
        <to-activity-id>SCM_0001</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__16">
        <from-outcome>ERP-ACT-SCM_0002</from-outcome>
        <to-activity-id>SCM_0002</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__21">
        <from-outcome>ERP-ACT-SCM_0003</from-outcome>
        <to-activity-id>ExeCompanySEC_0003</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__24">
      <from-activity-id>SCM_0001_EDIT</from-activity-id>
      <control-flow-case id="__25">
        <from-outcome>ERPBACKCRUD</from-outcome>
        <to-activity-id>SCM_0001</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__27">
      <from-activity-id>SCM_0001</from-activity-id>
      <control-flow-case id="__28">
        <from-outcome>ERP-ACT-SCM_0001_EDIT</from-outcome>
        <to-activity-id>ExecuteWithParSCMPARAByCompany</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__29">
      <from-activity-id>ExecuteWithParSCMPARAByCompany</from-activity-id>
      <control-flow-case id="__30">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>ExecuteWithParamsCompanyWise</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__31">
      <from-activity-id>ExecuteWithParamsCompanyWise</from-activity-id>
      <control-flow-case id="__32">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>SCM_0001_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__17">
      <from-activity-id>SCM_0002</from-activity-id>
      <control-flow-case id="__18">
        <from-outcome>ERP-ACT-SCM_0002</from-outcome>
        <to-activity-id>ExecuteWithParSCMPARAByComp</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__19">
      <from-activity-id>ExecuteWithParSCMPARAByComp</from-activity-id>
      <control-flow-case id="__20">
        <from-outcome>ExecuteWithParams</from-outcome>
        <to-activity-id>SCM_0002_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__26">
      <from-activity-id>ExeCompanySEC_0003</from-activity-id>
      <control-flow-case id="__33">
        <from-outcome>ERP-ACT-SCM_0003</from-outcome>
        <to-activity-id>SCM_0003</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__34">
      <from-activity-id>SCM_0003</from-activity-id>
      <control-flow-case id="__35">
        <from-outcome>ERP-ACT-SCM_0003QUERY</from-outcome>
        <to-activity-id>ExeParaCompanyByCompID</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__36">
      <from-activity-id>ExeParaCompanyByCompID</from-activity-id>
      <control-flow-case id="__37">
        <from-outcome>ERP-ACT-SCM_0003_QUERY</from-outcome>
        <to-activity-id>ExeTermHeaderByCompany</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__38">
      <from-activity-id>ExeTermHeaderByCompany</from-activity-id>
      <control-flow-case id="__39">
        <from-outcome>ExeTermHeaderByCompany</from-outcome>
        <to-activity-id>SCM_0003_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__40">
      <from-activity-id>SCM_0003_EDIT</from-activity-id>
      <control-flow-case id="__41">
        <from-outcome>ERPBACKCRUD</from-outcome>
        <to-activity-id>SCM_0003</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__42">
        <from-outcome>ERP-ACT-SCM_0003EDITQUERY</from-outcome>
        <to-activity-id>ExePayTermByPaymentHeaderID</to-activity-id>
      </control-flow-case>
      <control-flow-case id="__47">
        <from-outcome>ERP-ACT-SCM_0003ADD</from-outcome>
        <to-activity-id>CreateWithParaPaySCM_0003</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__43">
      <from-activity-id>ExePayTermByPaymentHeaderID</from-activity-id>
      <control-flow-case id="__44">
        <from-outcome>ERP-ACT-SCM_0003EDITQUERY</from-outcome>
        <to-activity-id>SCM_0003_EDIT_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__45">
      <from-activity-id>SCM_0003_EDIT_EDIT</from-activity-id>
      <control-flow-case id="__46">
        <from-outcome>ERPBACKCRUD</from-outcome>
        <to-activity-id>SCM_0003_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <control-flow-rule id="__48">
      <from-activity-id>CreateWithParaPaySCM_0003</from-activity-id>
      <control-flow-case id="__49">
        <from-outcome>ERP-ACT-SCM_0003ADD</from-outcome>
        <to-activity-id>SCM_0003_EDIT_EDIT</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <visibility>
      <url-invoke-allowed/>
    </visibility>
  </task-flow-definition>
</adfc-config>
