                                      <af:outputFormatted value="#{item.bindings.SupplierShortCode.inputValue}"
                                                          id="of2" styleClass="ERPHeroDetailColor">
                                        <af:convertNumber groupingUsed="false"
                                                          pattern="#{bindings.ScmSupplierByCompanyRO.hints.SupplierShortCode.format}"/>
                                      </af:outputFormatted>
                                    </af:panelGroupLayout>
                                  </af:gridCell>
                                </af:gridRow>
                                <af:gridRow marginTop="5px" height="auto" id="gr3">
                                  <af:gridCell marginStart="5px" width="100%" marginEnd="5px" id="gc3">
                                    <af:panelGroupLayout id="pgl6" layout="horizontal">
                                      <af:activeImage id="ai1" source="#{resource['images:imgShortName.png']}"/>
