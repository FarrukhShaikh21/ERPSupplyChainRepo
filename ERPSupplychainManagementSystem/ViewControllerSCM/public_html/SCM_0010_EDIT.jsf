                  <af:gridCell marginStart="5px" width="50%" marginEnd="5px" id="gc11" halign="end">
                    <af:panelGroupLayout id="pgl12" layout="horizontal" halign="end">
                      <af:link id="l1" icon="#{resource['images:imgCloseDialog.png']}"
