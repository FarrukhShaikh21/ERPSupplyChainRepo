                                    maximumLength="#{bindings.CompanyShortCode.hints.precision}"
                                    shortDesc="#{bindings.CompanyShortCode.hints.tooltip}" id="it5" readOnly="true">
                        <f:validator binding="#{bindings.CompanyShortCode.validator}"/>
