                          <af:panelGroupLayout layout="vertical" styleClass="WBCardSize" id="pgl1">
                            <f:facet name="separator">
                              <af:spacer width="10" id="s1"/>
                            </f:facet>
                            <af:panelBox text="PanelBox1" id="pb1" showHeader="never" showDisclosure="false">
